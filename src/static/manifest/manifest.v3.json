{
  "short_name": "extensiontemplate",
  "name": "extensiontemplate",
  "manifest_version": 3,
  "version": "0.0.0",
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://yourdomain.com.au/*", "*://www.yourdomain.com.au/*"],
      "js": ["./content-script.js"]
    }
  ],
  "permissions": [],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./icons/icon-16.png",
    "32": "./icons/icon-32.png",
    "128": "./icons/icon-128.png",
    "192": "./icons/icon-128.png"
  }
}
