{
  "short_name": "extensiontemplate Dev",
  "name": "extensiontemplate Dev",
  "manifest_version": 2,
  "version": "0.0.0",
  "background": {
    "scripts": ["./background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://localhost/*","*://127.0.0.1/*"],
      "js": ["./content-script.js"]
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./icons/icon-16.png",
    "32": "./icons/icon-32.png",
    "128": "./icons/icon-128.png",
    "192": "./icons/icon-128.png"
  }
}
